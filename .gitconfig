[merge]
    tool = meld
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[user]
    name = Jeremy Morrison
[branch]
    autosetuprebase = always
[alias]
    st = status
    co = checkout
    cob = co -b
    bs = branch -la
    b = branch -l
    ll = log --stat --abbrev-commit
    lg = log --graph --pretty=format:'%Cred%h%Creset - %C(bold blue)<%an>%C(yellow)%d%Creset %s %Cgreen(%cr) %Creset' --abbrev-commit --date=relative
    last = for-each-ref --sort=-committerdate refs/heads/ --format='%(refname:short)' --count=10

